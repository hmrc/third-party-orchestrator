# microservice specific routes

POST        /session/validate                   uk.gov.hmrc.thirdpartyorchestrator.controllers.SessionController.getDeveloperForSession()
GET         /application                        uk.gov.hmrc.thirdpartyorchestrator.controllers.ApplicationController.queryDispatcher()
GET         /applications/:id                   uk.gov.hmrc.thirdpartyorchestrator.controllers.ApplicationController.getApplication(id: uk.gov.hmrc.apiplatform.modules.common.domain.models.ApplicationId)
GET         /applications/:id/developers        uk.gov.hmrc.thirdpartyorchestrator.controllers.ApplicationController.getVerifiedDevelopersForApplication(id: uk.gov.hmrc.apiplatform.modules.common.domain.models.ApplicationId)
